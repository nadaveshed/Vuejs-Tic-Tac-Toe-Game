A,EAAA,GAAA;AAEA,SARA,CASA;;;AACA,YAAA,CAAA,aAAA,EAAA;AACA,UAAA,MAAA,CAAA,IAAA,CAAA,MAAA,EAAA,KAAA,EAAA,KAAA,MAAA;AACA;;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,SAAA,EAAA,KAAA,SAAA;AACA,aAAA,UAAA,GAAA,CAAA,KAAA,UAAA,CAdA,CAeA;;AACA,aAAA,WAAA;AACA,aAAA,WAAA;AACA;AACA,KA3BA;AA4BA,IAAA,OA5BA,qBA4BA;AACA,UAAA,KAAA,UAAA,EAAA;AACA,aAAA,MAAA,GAAA,WAAA;AACA,OAFA,MAEA;AACA,aAAA,MAAA,GAAA,WAAA;AACA;AACA,KAlCA;AAmCA,IAAA,WAnCA,yBAmCA;AACA,UAAA,gBAAA,GAAA,CAAA,CAAA,CAAA,EAAA,CAAA,EAAA,CAAA,CAAA,EAAA,CAAA,CAAA,EAAA,CAAA,EAAA,CAAA,CAAA,EAAA,CAAA,CAAA,EAAA,CAAA,EAAA,CAAA,CAAA,EACA,CAAA,CAAA,EAAA,CAAA,EAAA,CAAA,CADA,EACA,CAAA,CAAA,EAAA,CAAA,EAAA,CAAA,CADA,EACA,CAAA,CAAA,EAAA,CAAA,EAAA,CAAA,CADA,EACA,CAAA,CAAA,EAAA,CAAA,EAAA,CAAA,CADA,EACA,CAAA,CAAA,EAAA,CAAA,EAAA,CAAA,CADA,CAAA;;AAGA,WAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,gBAAA,CAAA,MAAA,EAAA,CAAA,EAAA,EAAA;AACA,YAAA,UAAA,GAAA,gBAAA,CAAA,CAAA,CAAA,CAAA,CAAA,CAAA;AACA,YAAA,WAAA,GAAA,gBAAA,CAAA,CAAA,CAAA,CAAA,CAAA,CAAA;AACA,YAAA,UAAA,GAAA,gBAAA,CAAA,CAAA,CAAA,CAAA,CAAA,CAAA;;AACA,YAAA,KAAA,SAAA,CAAA,UAAA,KAAA,KAAA,SAAA,CAAA,WAAA,CAAA,IACA,KAAA,SAAA,CAAA,UAAA,KAAA,KAAA,SAAA,CAAA,UAAA,CADA,IAEA,KAAA,SAAA,CAAA,UAAA,KAAA,EAFA,EAEA;AACA,eAAA,QAAA,GAAA,IAAA;AAEA,eAAA,MAAA,GAAA,KAAA,SAAA,CAAA,UAAA,CAAA;AACA;AACA;AACA,KAnDA;AAoDA,IAAA,WApDA,yBAoDA;AACA,WAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,IAAA,CAAA,EAAA,CAAA,EAAA,EAAA;AACA,YAAA,KAAA,SAAA,CAAA,CAAA,KAAA,EAAA,EAAA;AACA;AACA;AACA;;AACA,WAAA,GAAA,GAAA,IAAA;AACA,KA3DA;AA4DA,IAAA,UA5DA,wBA4DA;AACA,WAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,IAAA,CAAA,EAAA,CAAA,EAAA,EAAA;AACA,aAAA,SAAA,CAAA,CAAA,IAAA,EAAA;AACA,aAAA,QAAA,GAAA,KAAA;AACA,aAAA,MAAA,GAAA,EAAA;AACA,aAAA,GAAA,GAAA,KAAA;AACA,QAAA,QAAA,CAAA,cAAA,eAAA,CAAA,GAAA,SAAA,GAAA,KAAA,SAAA,CAAA,CAAA,CAAA;AACA;AACA;AApEA,GAnBA;AAyFA,EAAA,OAzFA,qBAyFA;AAAA;;AACA,IAAA,MAAA,CAAA,EAAA,CAAA,MAAA,EAAA,UAAA,KAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,gBAAA,EAAA,KAAA;;AACA,MAAA,KAAA,CAAA,IAAA,CAAA,KAAA,EAAA,IAAA,EAAA,UAAA;AACA,KAHA;AAIA;AA9FA,CAAA","sourcesContent":["<template>\n\n  <div class=\"container\">\n    <div class=\"title\"> Tic tac toe</div>\n<!--    <p id=\"demo\">Player {{player}} turn</p>-->\n    <div class=\"game\">\n      <div id=\"cell0\" class=\"cell\" @click=\"play(0, false)\">{{ gameArray[0] }}</div>\n      <div id=\"cell1\" class=\"cell\" @click=\"play(1, false)\">{{ gameArray[1] }}</div>\n      <div id=\"cell2\" class=\"cell\" @click=\"play(2, false)\">{{ gameArray[2] }}</div>\n      <div id=\"cell3\" class=\"cell\" @click=\"play(3, false)\">{{ gameArray[3] }}</div>\n      <div id=\"cell4\" class=\"cell\" @click=\"play(4, false)\">{{ gameArray[4] }}</div>\n      <div id=\"cell5\" class=\"cell\" @click=\"play(5, false)\">{{ gameArray[5] }}</div>\n      <div id=\"cell6\" class=\"cell\" @click=\"play(6, false)\">{{ gameArray[6] }}</div>\n      <div id=\"cell7\" class=\"cell\" @click=\"play(7, false)\">{{ gameArray[7] }}</div>\n      <div id=\"cell8\" class=\"cell\" @click=\"play(8, false)\">{{ gameArray[8] }}</div>\n    </div>\n    <div class=\"gameResult\">\n      <div id=\"winner\" v-if=\"gameOver\"> Winner is {{winner}} </div>\n      <div v-if=\"tie\"> Game is Tie </div>\n    </div>\n    <button class=\"resetButton\" @click=\"resetBoard()\" v-if=\"gameOver || tie\">RESET BOARD</button>\n\n  </div>\n</template>\n\n<script>\nimport io from 'socket.io-client'\nconst socket = io(\"http://localhost:3000\")\nexport default {\n  name: 'App',\n  components: {\n  },\n  data() {\n    return {\n      t: 0,\n      gameArray: [\"\", \"\", \"\", \"\", \"\", \"\", \"\", \"\", \"\"],\n      currentTurn: \"\",\n      winner: \"\",\n      tie: false,\n      player: \"1\",\n      playerTurn: true,\n      gameOver: false,\n    }\n  },\n  mounted() {\n    this.test();\n    },\n  methods: {\n    // test(){\n    //   console.log(\"test\", this.gameArray[0])\n    //   this.gameArray[0] = \"X\"\n    // },\n\n    play(index, drawFromOther) {\n\n      if(!this.gameOver) {\n        if (this.playerTurn) {\n          this.gameArray[index] = \"X\"\n          this.$set(this.gameArray, index, \"X\");\n        } else {\n          this.gameArray[index] = \"O\"\n          this.$set(this.gameArray, index, \"O\");\n\n        }\n        // document.getElementById(`cell${index}`).innerHTML = this.gameArray[index];\n        if (!drawFromOther) {\n          socket.emit(\"play\", index, this.player)\n        }\n        console.log(\"content\", this.gameArray)\n        this.playerTurn = !this.playerTurn;\n        // this.playing();\n        this.checkForWin();\n        this.checkForTie();\n      }\n    },\n    playing() {\n      if(this.playerTurn) {\n        this.player = \" player 1\"\n      } else {\n        this.player = \" player 2\"\n      }\n    },\n    checkForWin() {\n      const winsCombinations = [[0, 1, 2], [3, 4, 5], [6, 7, 8],\n        [0, 3, 6], [1, 4, 7], [2, 5, 8], [0, 4, 8], [2, 4, 6]];\n\n      for (let i = 0; i < winsCombinations.length; i++) {\n        let firstIndex = winsCombinations[i][0];\n        let secondIndex = winsCombinations[i][1];\n        let thirdIndex = winsCombinations[i][2];\n        if(this.gameArray[firstIndex] == this.gameArray[secondIndex] &&\n            this.gameArray[firstIndex] == this.gameArray[thirdIndex] &&\n            this.gameArray[firstIndex] != \"\") {\n          this.gameOver = true;\n\n          this.winner = this.gameArray[firstIndex];\n        }\n      }\n    },\n    checkForTie(){\n      for (let i = 0 ; i<= 8 ; i++) {\n        if(this.gameArray[i] == \"\") {\n          return\n        }\n      }\n      this.tie = true;\n    },\n    resetBoard() {\n      for (let i=0; i<= 8; i++) {\n        this.gameArray[i] = \"\"\n        this.gameOver = false;\n        this.winner = \"\"\n        this.tie = false\n        document.getElementById(`cell${i}`).innerHTML = this.gameArray[i];\n      }\n    }\n  },\n  created() {\n    socket.on(\"play\", (index) => {\n      console.log(\"received index\", index)\n      this.play(index, true, \"player 1\")\n    })\n  }\n}\n</script>\n\n<style>\n* {\n  margin: 0;\n  padding: 0;\n  box-sizing: border-box;\n}\n\n.container {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  justify-content: center;\n}\n\n.game {\n  display: grid;\n  width: 300px;\n  height: 300px;\n  grid-template-columns: auto auto auto;\n}\n\n.cell {\n  display: flex;\n  width: 100px;\n  height: 100px;\n  align-items: center;\n  justify-content: center;\n  font-size: 3rem;\n  font-weight: bold;\n  border: 3px solid black;\n  transition: background 0.2s ease-in-out;\n}\n\n#cell0,\n#cell1,\n#cell2 {\n  border-top: none;\n}\n#cell0,\n#cell3,\n#cell6 {\n  border-left: none;\n}\n#cell6,\n#cell7,\n#cell8 {\n  border-bottom: none;\n}\n#cell2,\n#cell5,\n#cell8 {\n  border-right: none;\n}\n\n.resetButton {\n  border: 4px solid green;\n  padding: 10px 20px;\n  font-size: 1rem;\n  font-weight: bold;\n}\n</style>"],"sourceRoot":"src"}]}